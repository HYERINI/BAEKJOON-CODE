♥ 1단계 _ 10818번
(런타임에러(OutOfBounds)_컨테이너 또는 배열에서 할당된 경계를 넘어가는 접근 발생) 

#include <stdio.h>
int main() {
	int num;
	int min = 1000000, max = -1000000;
	int arr[1000000];

	scanf_s("%d", &num);

	for (int i = 0; i < num; i++) {
		scanf_s("%d", &arr[i]);
	}

	for (int j = 0; j < num; j++) {
		if (min > arr[j])
			min = arr[j];

		if (max < arr[j])
			max = arr[j];
	}

	printf("%d %d", min, max);
	return 0;
}

♥ 2단계 _ 2562번

#include <stdio.h>
int main(void) {
	int i = 0;
	int a, min = 0;
	int arr[100];

	for (; i < 9; i++) {
		scanf_s("%d", &arr[i]);
		if (min < arr[i]) {
			min = arr[i];
			a = i;
		}
	}

	printf("%d\n%d", min, a + 1);
}

♥ 3단계 _ 2577번

#include <stdio.h>
int main(void) 
{
	int a, b, c = 0;
	int num = 0;
	int arr[10] = { 0, };
	scanf_s("%d %d %d", &a, &b, &c);

	int ans = 1;
	ans = a * b * c;
	

	for (int i = 0; ans > 0; i++) {
		num = ans % 10;
		arr[num] += 1;
		ans /= 10;
	}
	for (int i = 0; i < 10; i++) {
		printf("%d\n", arr[i]);
	}

}

♥4단계 _ 3052번

#include <stdio.h>

int main(void) 
{
	int a, b, num = 0;
	int result = 0;
	int arr[10] = {0};
	
	for(int i = 0; i < 10; i++ ) {
		scanf_s("%d", &n);
		num %= 42;
		arr[i] = num;
	}
	
	for(int j = 0; j < 10; j++ ) {
		int c = 0;
		for (int k = 0; k < j; k++ ) {
			if( arr[j] == arr[k] ) 
			    c++;
		}
		if ( c == 0 ) 
		    result++;
	}
	printf("%d", result);
	
	return 0;
}

5단계 _ 8958번

#include <stdio.h>

int main(void)
{
	int j, n;
	int score;					
	int test;				

	scanf("%d", &test);

	for (int i = 0; i < test; i++) {
		char ox[80] = { 0, };
		scanf_s("%s", ox);
		
		score = 0;
		j = 0;
		num = 1;
		
		while (ox[j] != '\0') {
			if (ox[j] == 'O') {
				score += (num);
				num++;
			}
			else {
				num = 1;
			}
			j++;
		}
		printf("%d\n", score);
	}
	return 0;
}

6단계 _ 4344번

#include <stdio.h>
int main(void){
	int test; 
	int total = 0;
	float num;
	int score[1000] = {0,};
	
	float avg = 0;
	float over = 0;
	float percent[1000] = {0.0,};
	
	scanf_s("%d", &test);
	
	for (int i = 0; i < test; i++){
		scanf_s("%f", &num);
		
		total = 0
		for (int j = 0; j < num; j++){
			scanf_s("%d", &score[j]);
			total += score[j];
		}
		
		avg = total / num;
		over = 0;
		for (int k = 0; k < num; k++){
			if (score[k] > avg){
				over++;
			}
			percent[i] = over / num * 100;
		}
	}
	
	for (int q = 0; q < test; q++){
		printf("%3.f%%\n", percent[q]);
	}
}
