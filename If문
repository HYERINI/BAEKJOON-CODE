♥ 1단계 _ 1330번

#include <stdio.h>
int main(void)
{
	int a, b;
	scanf_s("%d %d", &a, &b);
	if (a > b)
		printf(">");
	else if (a < b)
		printf("<");
	else
		printf("==");
}

♥ 2단계 _ 9498번

#include <stdio.h>
int main(void)
{
	int a;
	scanf_s("%d", &a);

	if (90 <= a && a <= 100)
		printf("A");
	else if (80 <= a && a <= 89)
		printf("B");
	else if (70 <= a && a <= 79)
		printf("C");
	else if (60 <= a && a <= 69)
		printf("D");
	else
		printf("F");
}

♥ 3단계 _ 2753번

#include <stdio.h>
int main(void)
{
	int year;
	scanf_s("%d", &year);
	if (year % 4 == 0 && year % 100 != 0)
		printf("1");
	else if (year % 400 == 0)
		printf("1");
	else
		printf("0");
}

♥ 4단계 _ 14681번

#include <stdio.h>
int main(void)
{
	int x, y;
	scanf_s("%d", &x);
	scanf_s("%d", &y);
	if (x >= 0 && y >= 0)
		printf("1");
	else if (x <= 0 && y >= 0)
		printf("2");
	else if (x >= 0 && y <= 0)
		printf("4");
	else
		printf("3");
}

♥ 5단계 _ 2884

#include <stdio.h>
int main(void)
{
	int min, sec;
	scanf_s("%d %d", &min, &sec);
	if (min == 0 && sec < 45)
		printf("%d %d", 23, 15 + sec);
	else if (min >= 0 && sec >= 45)
		printf("%d %d", min, sec - 45);
	else if (min > 0 && sec < 45)
		printf("%d %d", min - 1, 15 + sec);
}
