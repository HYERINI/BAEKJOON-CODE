}

♥9단계 _ 10869번

#include <stdio.h>
int main(void) {
	int a, b;
	scanf("%d %d", &a, &b);
	printf("%d\n%d\n%d\n%d\n%d", a + b, a - b, a * b, a / b, a % b);
}

♥10단계 _ 10430번

#include <stdio.h>
int main(void) {
	int a, b, c;
	scanf("%d %d %d", &a, &b, &c);
	printf("%d\n%d\n%d\n%d", (a + b) % c, ((a % c) + (b % c)) % c, (a * b) % c, ((a % c) * (b % c)) % c);
}

♥11단계 _ 2588번

#include <stdio.h>
int main(void)
{
	int a, b;
	scanf("%d", &a);
	scanf("%d", &b);
	int first = b / 100;
	int second = (b - 100 * first) / 10;
	int third = b - 100 * first - 10 * second;

	printf("%d\n%d\n%d\n%d", a * third, a * second, a * first, a * third + a * second * 10 + a * first * 100);
}
