♥ 6단계 _ 2742번
#include <stdio.h>
int main(void) {
	int num;
	scanf("%d", &num);

	for (int i = num; i > 0; i--)
	{
		printf("%d\n", i);
	}
}

♥ 7단계 _ 11021번
#include <stdio.h>
int main(void)
{
	int num, a, b;
	scanf("%d\n", &num);

	for (int i = 1; i <= num; i++)
	{
		scanf("%d %d", &a, &b);
		printf("Case #%d: %d\n", i, a + b);
	}
}

