♥ 1단계 _ 2739번
#include <stdio.h>
int main(void)
{
	int num;
	scanf("%d", &num);
	for (int i = 1; i < 10; i++)
	{
		printf("%d * %d = %d\n", num, i, num * i);
	}
}

♥ 2단계 _ 10950번
#include <stdio.h>
int main(void)
{
	int a, b, answer;
	scanf("%d\n", &answer);

	for (int i = 0; i < answer; i++)
	{
		scanf("%d %d", &a, &b);
		printf("%d\n", a + b);
	}
}

♥ 3단계 _ 8393번
#include <stdio.h>
int main(void) {
	int num, sum = 0;
	scanf("%d", &num);
	for (int i = 1; i <= num; i++) {
		sum += i;
	}
	printf("%d", sum);    
}

