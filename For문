		printf("Case #%d: %d + %d = %d\n", i, a, b, a + b);

♥ 9단계 _ 2438번
#include <stdio.h>
int main(void) {
    int num;
    scanf("%d", &num);
    
    for (int i = 0; i < num; i++) {
        for (int j = 0; j <= i; j++) {
            printf("*");
        }
        printf("\n");
    }
}

♥ 10단계 _ 2439번
#include <stdio.h>
int main(void) {
	int num;
	scanf("%d", &num);

	for (int i = 0; i < num;i++) {
		for (int space = 1; space + i < num; space++) {
			printf(" ");
		}
		for (int star = 0; star <= i; star++) {
			printf("*");
		}
		printf("\n");
	}
}

♥ 11단계 _ 10871번
#include <stdio.h>
int main(void) {
	int num, max;
	int answer;
	scanf("%d %d\n", &num, &max);

	for (int i = 0; i < num; i++) {
		scanf("%d", &answer);
		if (answer < max)
			printf("%d ", answer);
	}
}


